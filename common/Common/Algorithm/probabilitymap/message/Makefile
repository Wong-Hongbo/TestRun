ifeq ($(shell uname -i),x86_64)
   ifeq ($(SYSTEMNUM),18.04)
$(warning "the platform ubuntu $(SYSTEMNUM) $(shell uname -i)")
LIBS = $(DEVP_LIB_DIR)/libUGVAuto18.a
BINS =

all: $(LIBS)

.PHONY: all

# Rules for specific targets
# UGVAuto.a
$(DEVP_LIB_DIR)/libUGVAuto18.a: $(OBJS)
	$(AR) cr $@ $^
	$(RANLIB) $@
#	-\rm -f $(OBJS)
%n.cc : %n.hh
	$(NML_CODEGEN) $^ -o $@

clean1:
	-rm -f *.o
.PHONY: clean1

clean:
	-\rm -f *n.cc *.o lib*.a lib*.so lib*.la *.lib *.ddll
	-\rm -f ../../lib/libUGVAuto18.a
   endif
